PORT=5000
# PostgreSQL Database URL (Neon + PgBouncer - use for connection stability)
# Format: postgresql://USER:PASSWORD@HOST/DB?sslmode=require&pgbouncer=true&connection_limit=1
DATABASE_URL=postgresql://USER:PASSWORD@HOST/DB?sslmode=require&pgbouncer=true&connection_limit=1
JWT_SECRET=your_super_secret_jwt_key_change_in_production
JWT_EXPIRE=7d
NODE_ENV=development
